{
  "name": "landing-page-in-11ty",
  "version": "1.0.0",
  "description": "A simple landing page built with 11ty and Tailwind CSS",
  "main": "index.js",
  "scripts": {
    "build": "npx tailwindcss -i ./src/css/main.css -o ./_site/css/styles.css && npx @11ty/eleventy",
    "start": "npx @11ty/eleventy --serve & npx tailwindcss -i ./src/css/main.css -o ./_site/css/styles.css --watch",
    "dev": "concurrently \"npx @11ty/eleventy --serve\" \"npx tailwindcss -i ./src/css/main.css -o ./_site/css/styles.css --watch\""
  },
  "keywords": [
    "11ty",
    "tailwindcss",
    "landing-page"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5"
  }
}
